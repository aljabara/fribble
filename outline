
	reference is sonnet-number.line-number

        words.this = map.reduce(
                map.apply(reader(filename,.user.marker),.tokens),
                .isword)
        words.this = map.reduce(words.this,map.unique(words.this?))

        map.forall(words.this,.reference.add)
